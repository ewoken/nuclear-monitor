{
  "name": "nuclear-monitor",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://nuclear-monitor.fr",
  "dependencies": {
    "antd": "^3.22.0",
    "babel-plugin-import": "^1.12.0",
    "customize-cra": "^0.5.0",
    "leaflet": "^1.5.1",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "prop-types": "^15.7.2",
    "qs": "^6.8.0",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-leaflet": "^2.4.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.1",
    "recharts": "^1.8.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "fix": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" --fix .",
    "start": "react-app-rewired start",
    "build": "./scripts/build.sh",
    "doBuild": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build --message \"[ci skip] Circle CI deploy\" --user \"Circle CI <circleci@circleci.org>\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --no-ignore"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
